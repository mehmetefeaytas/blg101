#include <stdio.h>
main(){
    int x, y;
    for(x=1; x<=5; x++){
        for(y=1; y<=5-x; y++){
            printf(" ");
        }
        for(y=1; y<=2*x-1; y++){
            printf("*");
        }
        printf("\n");
    }
    for(x=4; x>=1; x--){
        for(y=1; y<=5-x; y++){
            printf(" ");
        }
        for(y=1; y<=2*x-1; y++){
            printf("*");
        }
        printf("\n");
    }
}