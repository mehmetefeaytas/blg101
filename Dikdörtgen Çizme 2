#include <stdio.h>

int main() {
    int satir, sutun;

    // Kullanıcıdan satır ve sütun uzunluğunu al
    printf("Satir sayisini giriniz: ");
    scanf("%d", &satir);
    printf("Sutun sayisini giriniz: ");
    scanf("%d", &sutun);

    // Alanı * ile kapla
    for (int i = 0; i < satir; i++) {
        for (int j = 0; j < sutun; j++) {
            printf("*");
        }
        printf("\n");
    }

    return 0;
}
