#include <stdio.h>

int main() {
    int x, y, z, enb;
    printf("3 sayi giriniz: ");
    scanf("%d %d %d", &x, &y, &z);

    if (x > y) {
        if (x > z) {
            enb = x;
        } else {
            enb = z;
        }
    } else {
        if (y > z) {
            enb = y;
        } else {
            enb = z;
        }
    }
    printf("En büyük sayi: %d\n", enb);
}
